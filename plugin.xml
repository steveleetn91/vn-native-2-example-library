<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        id="vn-native-2-example-library" version="0.2.3">
    <name>Vn Native example library</name>
    <description>Cordova Device Plugin</description>
    <license>Apache 2.0</license>
    <keywords>vnnative,example library</keywords>
    <js-module src="www/example-library.js" name="examplelibrary">
        <clobbers target="examplelibrary" />
    </js-module>
    <platform name="ios">
        <config-file target="config.xml" parent="/*">
            <feature name="Echo">
                <param name="ios-package" value="Echo" />
                <param name="onload" value="true" />
            </feature>
        </config-file>
        <header-file src="src/ios/Echo.h" />
        <source-file src="src/ios/Echo.m" />
    </platform>
</plugin>